    "db": "ts-node -r tsconfig-paths/register --project ./tsconfig.server.json ./node_modules/typeorm/cli.js --config ./apps/server/src/ormconfig.ts",
    "db.m.new": "yarn db migration:generate -p -n",
    "db.m.run": "yarn db migration:run",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "nest build && nest build dependencyResolver && razzle build",
    "prebuild": "rimraf dist",
    "start.debug": "nest start --debug --watch",
    "start.dev": "nest start --watch",
    "start.prod": "node dist/apps/server/main",
    "start.web": "razzle start",
    "start": "nest start && nest start dependencyResolver && razzle start",
    "test.cov": "jest --coverage",
    "test.debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test.e2e": "jest --config ./apps/server/test/jest-e2e.json",
    "test.watch": "jest --watch",
    "test": "jest"
